@Model IEnumerable <_1000Kitap.Identity.ApplicationUser>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <div class="fa-cpanel panel-primary">
        <div class="panel-heading">
            Users
        </div>
        <table class="fa-table table-striped">
            <tr>
                <th>Id</th>
                <th>Username</th>
                <th>Password</th>
                <th>Name</th>
                <th>Email</th>
            </tr>
            @if (Model==null)
            {
                <tr>
                    <td colspan="5" class="text-center">
                        Kullanıcı Yok
                    </td>
                </tr>
            }
            else
            {
                foreach (var item in Model)
                {
                    <tr>@item.Id</tr>
                    <tr>@item.UserName</tr>
                    <tr>@item.Password</tr>
                    <tr>@item.Name</tr>
                    <tr>@item.Email</tr>

                }
            }
        </table>
    </div>
